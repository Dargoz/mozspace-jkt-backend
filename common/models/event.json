{
  "name": "Event",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "startDateTime": {
      "type": "date",
      "required": true
    },
    "endDateTime": {
      "type": "date",
      "required": true
    },
    "isPaid": {
      "type": "boolean",
      "required": true
    },
    "isProvidingFood": {
      "type": "boolean",
      "required": true
    },
    "isUsingTable": {
      "type": "boolean",
      "required": true
    },
    "additionalInformation": {
      "type": "string",
      "required": false
    },
    "numberOfAttendees": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "roomType": {
      "type": "belongsTo",
      "model": "RoomType",
      "foreignKey": ""
    },
    "participants": {
      "type": "hasMany",
      "model": "Participant",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      },
      "through": "ListOfAttendees"
    }
  },
  "acls": [],
  "methods": {}
}
